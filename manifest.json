{
	"name": "Plex Player",
	"version": "0.1",
	"description": "Manage plex without loading the site",
	"icons": { "128": "images/icon_128.png" },
	"browser_action": {
		"default_icon": {
			"38": "images/icon_38.png"
		},
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage",
		"notifications"
	],
	"options_page": "options.html",
	"manifest_version": 2
}
